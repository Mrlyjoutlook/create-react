{
  "name": "react",
  "version": "1.0.0",
  "description": "react redux app project",
  "scripts": {
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "compile": "better-npm-run compile",
    "mock": "better-npm-run mock",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "clean": "rimraf dist/*"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore public --ignore src",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "mock": {
      "command": "cd mock && node mockServer",
      "env": {}
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "0.0.15",
    "compression": "^1.6.2",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "debug": "^2.6.8",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "happypack": "^4.0.0-beta.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "ip": "^1.1.5",
    "json-server": "^0.10.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "lodash-webpack-plugin": "^0.11.3",
    "nodemon": "^1.11.0",
    "nyan-progress-webpack-plugin": "^1.2.0",
    "os": "^0.1.1",
    "postcss-loader": "^1.3.3",
    "postcss-pxtorem": "^4.0.1",
    "redbox-react": "^1.3.6",
    "rimraf": "^2.6.1",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "vconsole-webpack-plugin": "^1.1.2",
    "webpack": "^3.0.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-manifest-plugin": "^1.1.0",
    "yargs": "^8.0.1"
  },
  "author": "mrlyj",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
